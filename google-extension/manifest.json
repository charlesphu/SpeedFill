{
  "manifest_version": 3,
  "name": "SpeedFill",
  "version": "1.0",
  "description": "Seamless Job Application Filler",
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "permissions": ["activeTab", "scripting", "storage", "tabs"]
}
